{
  "name": "tjc-portal-server",
  "version": "0.0.0",
  "private": true,
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node app.ts",
    "copy-files": "cp -r src/public/ dist/public/ && cp -r src/views/ dist/views/",
    "build": "tsc && npm run copy-files",
    "start": "node dist/app.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    }
  }
}
